/**************************************************
   main layout
**************************************************/
#wrap { position: relative; width: 100%; height: 100%; overflow: hidden;}
#side { position: absolute; width: 48px; top: 0; left: 0; bottom: 0; background: #333948 url('../Images/side.png') no-repeat top;}
#container {position: absolute;top: 0;left: 48px;right: 0;bottom: 0;}
#container .header {width: 100%;height: 94px;background: #1c90fb;}

.t{position:relative;}
.t:after{
    content:"";
    position:absolute;
    top: 50%;
    left: 50%;
    width: 10px;
    height: 10px;
    margin: -10px 0 0 -10px;
    background: transparent;
    border: 5px solid red;
    border-radius: 20px;
    opacity: 0.3;
    -webkit-animation-iteration-count: infinite;
    animation-iteration-count: infinite;
    -webkit-animation-duration:   1s;
    animation-duration:   1s;
    -webkit-animation-fill-mode:both;
    animation-fill-mode:both;
    -webkit-animation-name: orbit;
    animation-name: orbit;
    -webkit-animation-timing-function: linear;
    animation-timing-function: linear;
    -webkit-animation-name: orbit;
    animation-name: orbit;
    -webkit-animation-timing-function: linear;
    animation-timing-function: linear;
}

@keyframes orbit{
    0% {transform: rotate(0deg) translateX(2px) rotate(0deg);}
	100% {transform: rotate(360deg) translateX(2px) rotate(-360deg);}
}
@-webkit-keyframes orbit{
    0% {-webkit-transform: rotate(0deg) translateX(2px) rotate(0deg);}
	100% {-webkit-transform: rotate(360deg) translateX(2px) rotate(-360deg);}
}

