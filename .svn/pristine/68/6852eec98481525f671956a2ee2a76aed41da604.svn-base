package neos.cmm.kendo;

public class KTreeMenuItem extends KItemBase{
	private String name = null;
	private String menuGubun = null;
	private String menuClass = null;
	private String urlPath = null;
	private String urlGubun = null;
	
	public KTreeMenuItem(String seq, String parentSeq, String name, String urlPath, boolean expanded, String spriteCssClass, String menuGubun, String menuClass) {
		super.seq = seq;
		super.parentSeq = parentSeq;
		this.name = name;
		this.urlPath = urlPath;
		super.expanded = expanded;
		this.menuGubun = menuGubun;
		this.menuClass = menuClass;
		super.spriteCssClass = spriteCssClass;
	}
	
	public String getMenuClass() {
		return menuClass;
	}
	
	public void setMenuClass(String menuClass) {
		this.menuClass = menuClass;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getMenuGubun() {
		return menuGubun;
	}
	public void setMenuGubun(String menuGubun) {
		this.menuGubun = menuGubun;
	}
	public String getUrlPath() {
		return urlPath;
	}
	public void setUrlPath(String urlPath) {
		this.urlPath = urlPath;
	}
	public String getUrlGubun() {
		return urlGubun;
	}
	public void setUrlGubun(String urlGubun) {
		this.urlGubun = urlGubun;
	}
	
	
}
