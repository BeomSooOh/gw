package api.fax.model;

public class FaxCoverInfo {
	/**
	 * 표지 사용 여부 (1 : 사용, 0 : 미사용)
	 */
	private String CoverUse;
	/**
	 * 표지 내용
	 */
	private String CoverMemo;
	
	public String getCoverUse() {
		return CoverUse;
	}
	public void setCoverUse(String coverUse) {
		CoverUse = coverUse;
	}
	public String getCoverMemo() {
		return CoverMemo;
	}
	public void setCoverMemo(String coverMemo) {
		CoverMemo = coverMemo;
	}
}
