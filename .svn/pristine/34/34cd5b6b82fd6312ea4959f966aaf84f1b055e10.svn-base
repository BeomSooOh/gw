@charset "utf-8";

/*----------------------------*
	공통
*----------------------------*/
.portletBg{background:#f0f3f3;}
.freeb_main_bg.portal_ceo {background:#e1e2e3;}
.freeb_main_bg .portal_wrap {background:#F3F4F6 url('../Images/main/main_bg.jpg') no-repeat center -54px;}
.freeb_main_bg.portal_ceo .portal_wrap {background:#e1e2e3 url('../Images/main/bg_hd01.png') no-repeat center top;}

.portletGridWrap{margin:0 0 100px 0;}
.portletGrid{position:relative;}
.portletGrid.nonGrid > table{display:none}
.portletGrid.nonGrid:after{display:none}
.portletGrid.nonGrid:before{display:none}
.portletGrid.nonGrid .setting{display:none;}
.portletGrid.grid8{width:952px;margin:0 auto;}
.portletGrid.grid10{width:1188px;margin:0 auto;}
.portletGrid:after{content:"";position:absolute;top:0;bottom:0;left:50%;margin:0 0 0 -2px;border:2px dashed #ff5722;z-index:120;}
.portletGrid:before{content: "CENTER";position: absolute; top: 0px; left: 50%; padding: 5px; border-radius: 4px; margin: 0 0 0 -29px; background: #ff5722; color: #ffffff; z-index: 150;}
.portletGrid table{background:#f0f3f3;table-layout: auto;border-collapse: separate;border-spacing: 8px;}
.portletGrid table td{width: 108px;height: 108px;border:1px solid #dcdcdc;}
.portletGrid table td.setGrid{border:1px dashed #2196f3;background:#e6f4ff;}
.portletGrid table td.nowGrid.setGrid{border:1px dashed #2196f3;background:#e6f4ff;}
.portletGrid table td.nowGrid{border:1px solid #dcdcdc;background:#eaeaea;}

/*포틀릿 타입별 박스정의*/
.portletGrid .portletBox{position:absolute;background:#ffffff;z-index:5;transition: all 0.1s;-webkit-transition: all 0.1s;border:1px solid #d0dee9;box-sizing:border-box;}
.portletGrid .iframeBox{position:absolute;z-index:5;transition: all 0.1s;-webkit-transition: all 0.1s;border:1px solid #d0dee9;box-sizing:border-box;}
.portletGrid .titleBox{position:absolute;background:#ffffff;z-index:5;transition: all 0.1s;-webkit-transition: all 0.1s;border:1px solid #d0dee9;box-sizing:border-box;}
.portletGrid .titleBox h2{position:relative;color:#000000;font-size:0px;height: 29px;border-bottom:1px solid #e7e7e7;margin:0;padding:9px 0 0 14px;font-family: NSR;background:#f1faff;}
.portletGrid .titleBox h2 span{overflow: hidden;text-overflow: ellipsis;white-space: nowrap;display: inline-block;margin-left:6px;font-size:14px;}
.portletGrid .titleBox h2 span:after{content:"";display:block;clear:both;height:0;visibility:hidden;}
.portletGrid .titleBox h2 span.barline{float:left;color:#f1f1f1;margin:0 6px 0 0;}

.portletGrid .titleBox h2 a.more{display:block;position:absolute;top:16px;right:9px;width:7px;height:12px;background:url('../../Images/freeb/portlet_titleBox_more.png') no-repeat;}
.portletGrid .titleBox h2.ma{background: #f1faff url('../../Images/freeb/portlet_tit_s.png') no-repeat 14px 0px;padding: 14px 0 0 42px;}   /*메일*/
.portletGrid .titleBox h2.sc{background: #f1faff url('../../Images/freeb/portlet_tit_s.png') no-repeat 14px -44px;padding: 14px 0 0 42px;} /*일정*/
.portletGrid .titleBox h2.ea{background: #f1faff url('../../Images/freeb/portlet_tit_s.png') no-repeat 14px -88px;padding: 14px 0 0 42px;} /*전자결재*/
.portletGrid .titleBox h2.bd{background: #f1faff url('../../Images/freeb/portlet_tit_s.png') no-repeat 14px -132px;padding: 14px 0 0 42px;}/*게시판*/
.portletGrid .titleBox h2.su{background: #f1faff url('../../Images/freeb/portlet_tit_s.png') no-repeat 14px -176px;padding: 14px 0 0 42px;}/*설문조사*/
.portletGrid .titleBox h2.nt{background: #f1faff url('../../Images/freeb/portlet_tit_s.png') no-repeat 14px -220px;padding: 14px 0 0 42px;}/*노트*/
.portletGrid .titleBox h2.rm{background: #f1faff url('../../Images/freeb/portlet_tit_s.png') no-repeat 14px -264px;padding: 14px 0 0 42px;}/*받은편지함*/
.portletGrid .titleBox h2.ed{background: #f1faff url('../../Images/freeb/portlet_tit_s.png') no-repeat 14px -308px;padding: 14px 0 0 42px;}/*결재현황*/
.portletGrid .titleBox h2.wk{background: #f1faff url('../../Images/freeb/portlet_tit_s.png') no-repeat 14px -352px;padding: 14px 0 0 42px;}/*업무*/
.portletGrid .titleBox h2.exp{background: #f1faff url('../../Images/freeb/portlet_tit_s.png') no-repeat 14px -396px;padding: 14px 0 0 42px;}/*지출*/
.portletGrid .titleBox h2.cd{background: #f1faff url('../../Images/freeb/portlet_tit_s.png') no-repeat 14px -440px;padding: 14px 0 0 42px;}/*카드사용내역*/
.portletGrid .titleBox h2.ws{background: #f1faff url('../../Images/freeb/portlet_tit_s.png') no-repeat 14px -484px;padding: 14px 0 0 42px;}/*근무현황,신청*/
.portletGrid .titleBox h2.wt{background: #f1faff url('../../Images/freeb/portlet_tit_s.png') no-repeat 14px -528px;padding: 14px 0 0 42px;}/*근무시간*/

/*포틀릿 범위를 넘어가는 컨텐츠는 히든처리*/
.portletGrid .portletBox .ptl_content{overflow:hidden;}
.portletGrid .iframeBox .ptl_content{overflow:hidden;}
.portletGrid .titleBox .ptl_content{overflow:hidden;}

/*포틀릿에 컨텐츠가 없을때*/
.portletGrid .portletBox .ptl_content.nocon{background:#ffffff url('../../Images/freeb/portlet_nodata.png') no-repeat center center;}
.portletGrid .iframeBox .ptl_content.nocon{background:#ffffff url('../../Images/freeb/portlet_nodata.png') no-repeat center center;}
.portletGrid .titleBox .ptl_content.nocon{background:#ffffff url('../../Images/freeb/portlet_nodata.png') no-repeat center center;}

/*포틀릿 애니메이션 disabled*/
.portletGrid .portletBox.noAnimated{transition: unset !important;-webkit-transition: unset !important;}
.portletGrid .iframeBox.noAnimated{transition: unset !important;-webkit-transition: unset !important;}
.portletGrid .titleBox.noAnimated{transition: unset !important;-webkit-transition: unset !important;}
.portletGrid .portletBox.noAnimated .ptl_content{transition: unset !important;-webkit-transition: unset !important;}
.portletGrid .iframeBox.noAnimated .ptl_content{transition: unset !important;-webkit-transition: unset !important;}
.portletGrid .titleBox.noAnimated .ptl_content{transition: unset !important;-webkit-transition: unset !important;}

/* 포틀릿 누르고 있을때 
.portletGrid .portletBox:active{-webkit-animation:float 1.5s infinite; animation:float 1.5s infinite;z-index:100;}
.portletGrid .iframeBox:active{-webkit-animation:float 1.5s infinite; animation:float 1.5s infinite;z-index:100;}
.portletGrid .titleBox:active{-webkit-animation:float 1.5s infinite; animation:float 1.5s infinite;z-index:100;}
*/

/*포틀릿 설정레이어*/
.portletGrid .setting{position:absolute;top:0;left:0;right:0;bottom:0;cursor:move;z-index:10;background:url('../../Images/freeb/portlet_setting_bg.png');}
.portletGrid .setting .ptl_info .set_btn{display: block;float: right;width:15px;height:15px;margin: 0 0 7px 0;background:url('../../Images/freeb/portlet_setting.png') no-repeat center center;}
.portletGrid .setting .del_btn{position: absolute;top: 10px;right: 10px;width: 15px;height: 15px;background: url('../../Images/freeb/portlet_delete.png') no-repeat center center;}
.portletGrid .setting .ptl_info{position:absolute;bottom: 10px;right:10px;left: 10px;text-align: right;}
.portletGrid .setting .ptl_info .name{display:block;clear: both;font-size: 14px;color: #fff;font-family: NSB;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
.portletGrid .col1 .setting .ptl_info .name{display:block;clear: both;font-size: 14px;color: #fff;font-family: NSB;overflow: hidden;line-height:16px;max-height:48px;white-space: normal;text-overflow: clip;}


/*포틀릿그리드 스크롤커스텀*/
.portletGridWrap .mCSB_scrollTools{z-index:10}
.portletGridWrap.freebScroll .mCSB_scrollTools.mCSB_scrollTools_vertical .mCSB_draggerRail{width:4px;background-color: #000;background-color: rgba(0,0,0,0.4);}
.portletGridWrap.freebScroll .mCSB_scrollTools.mCSB_scrollTools_horizontal .mCSB_draggerRail{height:4px;margin:6px 0;background-color: #000;background-color: rgba(0,0,0,0.4);}
.portletGridWrap .mCSB_inside > .mCSB_container{margin-right:0 !important;}

/*포틀릿설정 버튼영역*/
.portlet_controll{position:absolute;right:16px;bottom:28px;}


/*----------------------------*
	PORTLET LIST
*----------------------------*/
.portletThumbnail{position:fixed;left:1px;right:1px;bottom:0px;height:100px;background:#4a4a4a}
.portletList{margin:0 auto;padding:0 16px 0 16px;}
.portletList.insize{margin:0 auto;padding:0 100px 0 16px;}
.portletList ul{white-space: nowrap;font-size:0;text-align:center;}
.portletList ul:after{content:"";display:block;clear:both;height:0;visibility:hidden;}
.portletList li{display:inline-block;padding:10px 4px 20px 4px;cursor:move;}
.portletList li span{display:table-cell;width:70px;max-width:70px;height:70px;padding:0;text-align:center;font-size:12px;font-family: sans-serif;font-weight: bold;color:#ffffff;background: #323232;border-radius: 4px;border: 2px solid #323232;vertical-align: middle;white-space: normal;box-sizing: border-box;}
.portletList li span em{display: block;font-family: sans-serif;font-weight: bold;font-size: 10px;color: #696969;transition: all 0.3s ease-in 0s;-webkit-transition: all 0.3s ease-in 0s;}
.portletList li:hover span{border: 2px solid #1a66c2;transition: all 0.2s ease-in 0.2s;-webkit-transition: all 0.2s ease-in 0.2s;-webkit-animation: float 0.3s;animation: float 0.3s;}
.portletList li.use:hover span{border: 2px solid #323232;-webkit-animation:none;animation:none;}
.portletList li:hover span em{color:#1a66c2;}
.portletList li.use:hover span em{color:#696969;}

.portletList li.use{position:relative;}
.portletList li.use:after{content: '등록불가';position: absolute;left: 0;right: 0;top: 6px;bottom: 16px;border-radius:4px;font-size: 12px;color: #fff;text-align: center;padding: 50px 0 0 0;background: rgba(200,0,0,0.5);cursor: not-allowed;}

.portletList .mCSB_horizontal.mCSB_inside > .mCSB_container{margin-bottom:0 !important;}
.portletList .mCSB_scrollTools.mCSB_scrollTools_horizontal{left:0px !important;right:0px !important;}

