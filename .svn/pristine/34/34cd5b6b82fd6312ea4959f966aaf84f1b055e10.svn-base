.exp_today{padding:10px 20px 0 20px;}
.exp_today ul {height:38px; border-bottom:1px solid #ebebeb;}
.exp_today ul li.img{float:left; width:120px;}
.exp_today ul li.text{float:left; width:245px; line-height:38px;}
.exp_today ul li.btn{float:right; padding:2px 4px; border-radius:2px; color:#fff; font-size:11px; margin:10px 0 0 0;}
.exp_today .blue{background:#56a8f4;}
.exp_today .red{background:#ee6362;}
.exp_today .gray{background:#bababa;}
.exp_more{float:right; font-size:13px; padding:0 20px 0 10px; color:#646464; background:url('../Images/ico/aexp_today_more.png') no-repeat left 3px;}

/*공지사항*/
.exp_notice{padding:10px 15px 0 19px; overflow:auto;}
.exp_notice ul li{line-height:26px; background:url('../Images/ico/ico_dot01.png') no-repeat left 11px; padding-left:7px; text-overflow:ellipsis;white-space:nowrap;overflow:hidden;}
.exp_notice ul li.new{font-weight:bold;}

/*주요 회계업무*/
.exp_major{padding:24px 17px 0 17px; font-family: NSB; line-height:32px;}
.exp_major ul li.list{float:left; text-overflow:ellipsis;white-space:nowrap;overflow:hidden;}
.exp_major ul li.day{float:right; width:40px; text-align:right;}
.exp_major ul li.red{color:#ee6362;}
.exp_major ul li.blue{color:#56a8f4;}
.exp_major .major01{float:left; width:25px; height:28px; background:url('../Images/main/exp_major01.png') no-repeat left 3px; padding:0 6px 0 0;}
.exp_major .major02{float:left; width:25px; height:28px; background:url('../Images/main/exp_major02.png') no-repeat left 3px; padding:0 6px 0 0;}
.exp_major .major03{float:left; width:25px; height:28px; background:url('../Images/main/exp_major03.png') no-repeat left 3px; padding:0 6px 0 0;}
.exp_major .major04{float:left; width:25px; height:28px; background:url('../Images/main/exp_major04.png') no-repeat left 3px; padding:0 6px 0 0;}

/*portletBox_link*/
.portletBox_link{padding:30px 0 0 25px;}
.portletBox_link p{clear:both;}
.portletBox_link a{float:left; width:80px; height:25px; margin:0 20px 0 0; padding:55px 0 0 0; text-align:center; font-family: NSB;}
.portletBox_link .link01{background:url('../Images/main/portletBox_link01.png') no-repeat center top;}
.portletBox_link .link02{background:url('../Images/main/portletBox_link02.png') no-repeat center top;}
.portletBox_link .link03{background:url('../Images/main/portletBox_link03.png') no-repeat center top;}
.portletBox_link .link04{background:url('../Images/main/portletBox_link04.png') no-repeat center top;}

/*받은편지함*/
