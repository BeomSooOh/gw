/* Table - Style in Drop&Down */
.dze_table_style_container {
	position: relative;
}
.dze_table_style_container > div {
	position: absolute;
	cursor:default;
	border: 1px solid #dedede;
	display: flex;
	justify-content: center;
	align-items: center;
}
.dze_table_style_container > div:hover {
	background-color:#deeefa;
}
.dze_table_style_container > div[selected] {
	background-color:#cdeaff;
}
.dze_table_style_container table {
	border-collapse: collapse;
	background-color: white;
}
.dze_table_style_container td {
	text-align: center;
	vertical-align: middle;
}

/* table style (UCDOC-4172) */
table.dze_table_style_2 tr:nth-child(2n) {
	background-color: #F2F2F2;
}

table.dze_table_style_3 tr:first-child {
	background-color: #D9D9D9;
}

table.dze_table_style_4 tr:nth-child(2n) {
	background-color: #BFBFBF;
}
table.dze_table_style_4 tr:nth-child(2n+1) {
	background-color: #D9D9D9;
}
table.dze_table_style_4 tr:first-child {
	background-color: #808080;
	color: #FFFFFF;
}

table.dze_table_style_5 tr:first-child {
	background-color: #808080;
	color: #FFFFFF;
}

table.dze_table_style_6 tr:first-child {
	background-color: #404040;
	color: #FFFFFF;
}
table.dze_table_style_6 tr:nth-child(2n) {
	background-color: #BFBFBF;
}

table.dze_table_style_7 tr:nth-child(2n) {
	background-color: #D9D9D9;
}
table.dze_table_style_7 tr:nth-child(2n+1) {
	background-color: #BFBFBF;
}
table.dze_table_style_7 tr:first-child {
	background-color: #404040;
	color: #FFFFFF;
}

table.dze_table_style_8 tr {
	background-color: #000000;
	color: #FFFFFF;
}

