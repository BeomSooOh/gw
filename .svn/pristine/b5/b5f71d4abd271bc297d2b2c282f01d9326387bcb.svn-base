.dze_ribbon_menu_title_bg > input {
	padding: 1px 5px 2px 5px;
	margin: 0 20px;
	font-family: "[더존] 본문체 30";
	font-size: 14px;
	font-weight: bold;
	text-align: center;
	color: white;
	background-color: transparent;
	border: 1px solid #6ea3f9;
}

.dze_ribbon_menu_title_bg > input[readonly] {
	cursor: default;
	border: none;
}

.dze_ribbon_menu_title_bg > input:not([readonly]):not(:active):not(:focus):hover {
	cursor: pointer;
}
.dze_ribbon_menu_title_bg > input:not([readonly]):hover {
    box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.2);
}

.dze_ribbon_menu_title_bg > input:not([readonly]):active,
.dze_ribbon_menu_title_bg > input:not([readonly]):focus {
	box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.2);
	background-color: #0476e0;
	border: 2px solid #6ea3f9;
}

.dze_ribbon_menu_title_bg > input::selection {
	color: black;
    background: #FFFFFF;
}

.dze_ribbon_menu_title_bg > input::-ms-clear {
	display: none;
}

