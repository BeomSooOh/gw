	<select id="OrgAdapterManage.selectEmpPasswdCheck" parameterClass="hashmap" resultClass="egovMap">
		select 
		a.group_seq,
		a.native_lang_code as lang_code,
		a.login_id,
		ifnull(a.erp_emp_num,'▦') as erp_emp_num,
		ifnull(a.mobile_tel_num,'▦') as mobile_tel_num,
		ifnull(DATE_FORMAT(a.bday, '%Y-%m-%d'),'0000-00-00') as bday,
		ifnull(prev_login_passwd,'▦') as prev_login_passwd,
		c.emp_seq,
		c.comp_seq,
		b.dept_seq,
		<isEqual property="type" compareValue="find">
		'Y' as checkYn		 
		</isEqual>
		<isNotEqual property="type" compareValue="find">
		case when (case #type# when 'def' then a.login_passwd
		when 'app' then (case when ifnull(a.appr_passwd,'')='' then a.login_passwd else a.appr_passwd end)
		else (case when ifnull(a.pay_passwd,'')='' then a.login_passwd else a.pay_passwd end)end) = #encPasswdOld# then 'Y' else 'N' end as checkYn		 
		</isNotEqual>				
		from $DB_NEOS$t_co_emp a
		join $DB_NEOS$t_co_emp_dept b on a.emp_seq = b.emp_seq
		join $DB_NEOS$t_co_emp_comp c on b.emp_seq = c.emp_seq and b.comp_seq = c.comp_seq
		where
		
		<isNotEmpty property="empSeq">
		a.emp_seq=#empSeq#
		</isNotEmpty>
		
		<isEmpty property="empSeq">
		a.login_id=#loginId#
		</isEmpty>		
		
		order by c.use_yn desc, c.work_status desc, (a.main_comp_seq = c.comp_seq) desc
		limit 1
	</select>
	
	
	
