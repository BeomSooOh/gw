    <select id="OrgAdapterManage.getEmpCompWorkUseY" parameterClass="hashmap" resultClass="string">
        SELECT COUNT(*)
        FROM $DB_NEOS$t_co_emp_comp
        WHERE emp_seq = #empSeq#
