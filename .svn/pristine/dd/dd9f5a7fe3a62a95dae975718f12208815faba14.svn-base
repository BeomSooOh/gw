/**
 *
 * 기본 글꼴에 대한 재정의.
**/

/* 크롬/엣지 버그로 인해서 기본 글꼴에 대해 재정의 하지 않음 UBA-97732 2022-06-20 */

html {
	width:auto;
	height:auto;
}

body, td, th {
	/* 호출페이지에서 기본폰트가 설정되어 있지 않은 경우 선언 */
	/* 호출페이지에서 선언되어 있는 경우는 주석처리 할 것 */
	font-family: '돋움', Dotum, '굴림', Gulim, sans-serif;
	font-size: 9pt;
}

body {
	margin: 3px;
	width:auto;
	height:auto;
	word-wrap: break-word;
	word-break: normal; /* UBA-14575 */
}

table, td, th {
	word-break: break-all; /* UBA-14575 */
}

.dze_table {
	table-layout: fixed; /* UCDOC-463 */
	word-wrap: break-word;
	word-break: normal;
}

.dze_table td, .dze_table th {
	word-wrap: break-word; /* UCDOC-463 */
	word-break: normal;
}

p, blockquote {
	margin-top:0px;
	margin-bottom:0px;
}

.dze_tb_layout {
	border-collapse:collapse;
}
.dze_tb_layout_td {
	border:1px dotted #c5c5c5 !important;
}
.dze_tb_layout_dummy {
	
}
.dze_tb_layout_td_dummy {
	
}

/* 내용이 있는 북마크 표식 */
.dze_bookmark_flag {
    margin-left: 2px;
    margin-right: 2px;
    padding: 0 0 0 8px;
    background: url('../image/toolbar/ico_bookmark.png') no-repeat 0 0;
}
.dze_bookmark_flag_dummy {
}

/* 초기 커서 위치 설정 표식 */
.dze_init_cursor_flag {
	margin-left: 2px;
	
    margin-right: 2px;
/*	width:14px;
	height:14px;
	display: inline-block;*/
	padding:2px 7px 5px 7px;
	background: url('../image/toolbar/ico_init_cursor.png') !important;
	background-repeat: no-repeat !important;
    background-position-x: 0;
    background-position-y: 0;
}

input[type=radio] {
	position:relative; 
	visibility:visible;
}

input[type=radio] + label {
	background: none;
}

input[type=radio]:checked + label {
	background: none;
}

input[type=radio]:disabled + label {
	background: none;
}

input[type=checkbox] {
	position:relative; 
	visibility:visible;
}
input::-webkit-inner-spin-button {
	appearance: none;
	-moz-appearance: none;
	-webkit-appearance: none;
}
.dze_printpagebreak {
	page-break-after:always;
}
.dze_printpagebreak_guide {
	width: 100%;
	margin: 10px 0 10px 0;
	height: 0;
	border-width: 3px 0 0 0;
	border-style: dashed;
	border-color: #d9d9d9;
}
.dze_printpagebreak_guide_dummy {
}

.dze_external_data_area {
	background: #FFFFAF;
	border: none !important;
	border-bottom:1px solid gray !important; 
	color: black;
	text-align: center;
	display: inline;
}

/* cell select (UCDOC-432) */
td[cusflag]:not([bg_changing]), th[cusflag]:not([bg_changing]) {
	background-image: url(../image/dialog/bg_selected_cell.png) !important;
	background-repeat: repeat !important;
}

input[dze_bind_key]:not(:active):not(:focus) {
	border:1px solid #A6C7FF !important; 
}

.tabSpan {
	white-space: pre;
    display: inline-block;
    text-indent: 0px;
    pointer-events: none;
}

body[contenteditable="true"] td[contenteditable="false"],	/* 셀 편집 제한 A10D-569 */
body[contenteditable="true"] th[contenteditable="false"] {
	position: relative;
}

body[contenteditable="true"] td[contenteditable="false"]:before,
body[contenteditable="true"] th[contenteditable="false"]:before {
	content: '';
	position: absolute;
	width: 14px;
	height: 14px;
	right: 2px;
	bottom: 2px;
	background-image: url(../image/toolbar/ico_incell_lock.png);
	background-repeat: no-repeat;
	background-position: 0 0;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	pointer-events: none;
}

