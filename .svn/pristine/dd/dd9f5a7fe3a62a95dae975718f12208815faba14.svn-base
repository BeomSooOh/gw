/* Amaranth 링크 */
.dze_oneffice_link_flag:before,
.dze_oneffice_slide_link_flag:before,
.dze_onechamber_link_flag:before,
.dze_amaranth_link:before {
	background: url('/modules/oneffice/image/ico_amaranth_link_file_s.png') no-repeat center center;
	content: " ";
	width: 10pt;
	height: 10pt;
	position: absolute;
	top: 0;
	left: -11pt;
}

.dze_oneffice_link_flag,
.dze_oneffice_slide_link_flag,
.dze_onechamber_link_flag,
.dze_amaranth_link {
	position: relative;
    margin: 0 0 0 11pt;
	color: #1C90FB !important;
	/*background: rgba(28,144,251,.07);*/ /* 원피스/웹에디터, 웹에디터뷰어 처리가 다르니 확인필요 */
}

.dze_oneffice_link_flag>span,
.dze_oneffice_slide_link_flag>span,
.dze_onechamber_link_flag>span,
.dze_amaranth_link>span {
	color: #1C90FB !important;
	background: rgba(28,144,251,.07) !important; /* 원피스/웹에디터, 웹에디터뷰어 처리가 다르니 확인필요 */
}

