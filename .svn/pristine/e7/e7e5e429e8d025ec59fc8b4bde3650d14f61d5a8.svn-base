#id_report_datepicker li.selected {font-size: 19px; font-weight: bold;}
#id_report_datepicker li.edge {font-size: 17px; opacity: 0.2;}
#id_report_datepicker li.middle {font-size: 18px; opacity: 0.5;}
#id_report_datepicker li.edge.past {transform: perspective(100px) rotateX(45deg);}
#id_report_datepicker li.middle.past {transform: perspective(100px) rotateX(30deg);}
#id_report_datepicker li.middle.future {transform: perspective(100px) rotateX(-30deg);}
#id_report_datepicker li.edge.future {transform: perspective(100px) rotateX(-45deg);}

