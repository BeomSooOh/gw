#id_datepicker {width: calc(100% - 16px); height: calc(100% - 6px); border: none; font-size: 16px; font-family: AppleSDGothicNeo, Noto sans, '[더존] 본문체 10', sans-serif;}
#id_report_datepicker td {font-family: AppleSDGothicNeo, Noto sans, '[더존] 본문체 10', sans-serif;}
