@charset "utf-8";

/**************************************************
  공통
**************************************************/
/*토글 버튼 (on/off기능)*/
.toggleBtn .toggleBG{background: #529bf8; width: 52px; height: 30px; border: 1px solid #529bf8; border-radius: 20px;}
.toggleBtn .toggleFG{float:left; background:#fff; width: 30px; height: 30px; border: none; border-radius: 15px; position: relative; left: 22px; box-shadow: -0.7px 3px 1.9px 2.1px rgba(0, 0, 1, 0.05);}

/*input*/
input::-ms-clear {display: none;}
input {outline:none;}

input[type="search"]::-webkit-search-cancel-button{ -webkit-appearance: none;width:20px;height:20px;background:url('../../images/btn/btn-input-cancel-none.png') no-repeat center center; background-size:20px 20px;}
input[type="search"]::-webkit-search-cancel-button:active {background:url('../../images/btn/btn-input-cancel-sele.png') no-repeat center center; background-size:20px 20px;}
input[type="search"]::-webkit-input-placeholder {color: #666; text-align:left;}

input[type="radio"]{outline:none;-webkit-appearance: none;float:left; vertical-align: middle; margin:0 7px 0 0; width:20px; height:20px; background:url('../../images/btn/btn-check-off.png') no-repeat center center;background-size:20px 20px;}
input[type="radio"]:active{width:20px; height:20px; background:url('../../images/btn/btn-check-on.png') no-repeat center center;background-size:20px 20px;}
.ui-radio-on+input[type="radio"]{background:url('../../images/btn/btn-check-on.png') no-repeat center center;background-size:20px 20px;}
.ui-btn.ui-radio-on.ui-btn-a:after{border-color:#38c;}
.ui-checkbox-on .ui-icon, .ui-radio-on .ui-icon{background-color:#4596ce;}

input[type="password"] {height: 48px; padding: 0px 40px 0px 13px; font-size: 16px; border-radius: 4px; border: solid 1px #dddddd;}
input[type="password"]:disabled {background: #eeeeee}

/*디렉토리 경로*/
.location{position:relative; height:49px; line-height:49px; padding:0 0 0 15px; border-bottom:1px solid #ddd; font-weight:500; overflow: hidden}
.location .location_iscroll {overflow: hidden;}
.location .location_iscroll ul li{float:left; background: url("../../images/ico/icon-path-arrow.png") no-repeat top 15px right 0px; background-size: 16px 16px; padding:0 20px 0 5px;}
.location .location_iscroll ul li:first-child{padding-left:5px; color:#333 !important;}
.location .location_iscroll ul li:last-child{background:none; color:#148dda; font-weight: bold;}
.location .location_iscroll em{color:#148dda; font-style:normal; padding:0 0 0 4px;}
.location .btn .btn_list {position: absolute; width: 16px; height: 16px; background: url("../../images/btn/btn-listview-none.png") no-repeat center center; background-size: 20px 20px; right: 57px; top: 15px;}
.location .btn .btn_list:active {background: url("../../images/btn/btn-listview-sele.png") no-repeat center center; background-size: 20px 20px;}
.location .btn .btn_win {position: absolute; width: 16px; height: 16px; background: url("../../images/btn/btn-thumbview-none.png") no-repeat center center; background-size: 20px 20px; right: 57px; top: 15px;}
.location .btn .btn_win:active {background: url("../../images/btn/btn-thumbview-sele.png") no-repeat center center; background-size: 20px 20px;}
.location .btn .btn_more {position:absolute; width:16px;height:16px;background: url("../../images/btn/btn-more-none.png") no-repeat center center; background-size: 20px 20px;right:17px; top:15px;}
.location .btn .btn_more:active {background:url("../../images/btn/btn-more-sele.png") no-repeat center center;background-size:20px 20px;}


/**************************************************
  설정
**************************************************/
.set dl{display:block; height:56px; line-height:56px; border-bottom:1px solid #dddddd;}
.set dl:active{background-color:#e5e6eb;}
.set dl dt{float:left; text-align:left; padding:0 0 0 60px;}
.set dl dd{float:right; text-align:right; padding:0 16px 0 0;}

.set dl.user dt{background:url('../../images/ico/icon-user.png') no-repeat 17px center; background-size:20px 20px;}
.set dl.alarm dt{background:url('../../images/ico/icon-alarm.png') no-repeat 17px center; background-size:20px 20px;}
.set dl.alarm dd{padding-top:12px;}
.set dl.info dt{background:url('../../images/ico/icon-info.png') no-repeat 17px center; background-size:20px 20px;}
.set dl.logout dt{background:url('../../images/ico/icon-logout.png') no-repeat 17px center; background-size:20px 20px;}
.set dl.logout dd{width:33px; height:56px; background:url('../../images/btn/btn-arrow-none.png') no-repeat 16px center; background-size:16px 16px;}
.set dl.logout:active dd{background:url('../../images/btn/btn-arrow-sele.png') no-repeat 16px center; background-size:16px 16px;}


/**************************************************
  사용자공유
**************************************************/
/*검색*/
.userSearch{clear:both; margin:0px 16px;}
.userSearch .searchbox{height:38px; padding:24px 0 0 0;}
.userSearch .btnOrg{width:20px; height:20px; background:url('../../images/btn/btn-org-none.png') no-repeat right center; background-size:20px 20px; padding-left: 3px; background-color: #ffffff; position: relative; top: 30px; float: right;}
.userSearch .btnOrg:active{background:url('../../images/btn/btn-org-sele.png') no-repeat right center; background-size:20px 20px;}

.selectField {display:inline-block; padding:3px 8px 3px 3px; height:32px; line-height:24px; box-sizing: border-box; border-radius:16px; box-shadow: 0px 2px 8px 0 rgba(0, 0, 0, 0.16); border: solid 1px #bababa; background:#fff; margin-right: 5px;}
.selectField.loading {visibility: hidden;}
.selectField.check {border: solid 1px #529bf8; background: #e8f2ff;}
.selectField .pic{width:24px !important; height:24px !important;}
.selectField .pic img{width:24px !important; height:24px !important; border-radius: 12px; vertical-align:top;}
.selectField .picbg{background-size:24px 24px !important}
.selectField .text{font-family: AppleSDGothicNeo, Noto sans, '[더존] 본문체 10', sans-serif; font-size: 16px; font-weight: normal; font-style: normal; font-stretch: normal; line-height: normal; letter-spacing: -0.8px; text-align: left; color: #000000;}

.writeField{width: 100%; display:inline-block;}
.writeField input{height:32px; border:none; outline:none;}

/*빈데이터 화면*/
.nodata{display:table; margin:0 auto; width:100%;}
.nodata .bg{display:table-cell; width:80px; height:92px; background:url("../../images/bg/img-empty-search.png") no-repeat center center; background-size:80px 92px;}

/*사용자공유 이미지공통*/
.userShare .pic{float:left; width:48px; height:48px;}
.userShare .pic img{width:48px; height:48px;}
.userShare .pic > img{border-radius:50% 50%;}

.userShare .bg01{background:url("../../images/bg/img-profile-none-01.png") no-repeat left top; background-size:48px 48px;}
.userShare .bg02{background:url("../../images/bg/img-profile-none-02.png") no-repeat left top; background-size:48px 48px;}
.userShare .bg03{background:url("../../images/bg/img-profile-none-03.png") no-repeat left top; background-size:48px 48px;}
.userShare .bg04{background:url("../../images/bg/img-profile-none-04.png") no-repeat left top; background-size:48px 48px;}
.userShare .bg05{background:url("../../images/bg/img-profile-none-05.png") no-repeat left top; background-size:48px 48px;}

.userShare .bg01.group{background:url("../../images/bg/img-profile-group-01.png") no-repeat left top; background-size:48px 48px;}
.userShare .bg02.group{background:url("../../images/bg/img-profile-group-02.png") no-repeat left top; background-size:48px 48px;}
.userShare .bg03.group{background:url("../../images/bg/img-profile-group-03.png") no-repeat left top; background-size:48px 48px;}
.userShare .bg04.group{background:url("../../images/bg/img-profile-group-04.png") no-repeat left top; background-size:48px 48px;}
.userShare .bg05.group{background:url("../../images/bg/img-profile-group-05.png") no-repeat left top; background-size:48px 48px;}


/*사원목록 화면*/
.userShare .listUser{width: 100%; max-height: 196px; clear:both; font-family: AppleSDGothicNeo, Noto sans, '[더존] 본문체 10', sans-serif; position: absolute; background: white; z-index: 11; border-bottom-left-radius: 7px; border-bottom-right-radius: 7px; box-shadow: 0px 3px 10px 1px black;}
.userShare .listUser table {table-layout: fixed;}
.userShare .listUser table tr{border-bottom:1px solid #ddd; height: 64px;}
.userShare .listUser table tr:active{background:#e5e6eb;}
.userShare .listUser .pic {position:relative; height:inherit; padding:0 0 0 20px;}
.userShare .listUser .pic.bg01 {background:url("../../images/bg/img-profile-none-01.png") no-repeat 20px 8px; background-size:48px 48px;}
.userShare .listUser .pic.bg02 {background:url("../../images/bg/img-profile-none-02.png") no-repeat 20px 8px; background-size:48px 48px;}
.userShare .listUser .pic.bg03 {background:url("../../images/bg/img-profile-none-03.png") no-repeat 20px 8px; background-size:48px 48px;}
.userShare .listUser .pic.bg04 {background:url("../../images/bg/img-profile-none-04.png") no-repeat 20px 8px; background-size:48px 48px;}
.userShare .listUser .pic.bg05 {background:url("../../images/bg/img-profile-none-05.png") no-repeat 20px 8px; background-size:48px 48px;}
.userShare .listUser .pic img{vertical-align:middle; margin-top:8px;}
.userShare .listUser dl{padding:0 0 0 10px;}
.userShare .listUser dl dt{overflow:hidden;}
.userShare .listUser dl dt {color:#000; overflow:hidden;}
.userShare .listUser dl dt em{color:#148dda; font-style:normal;}
.userShare .listUser dl dd {font-size:12px; color: #999; white-space:nowrap;overflow:hidden;text-overflow: ellipsis; padding:2px 16px 0 0;}

/*권한설정,메세지*/
.authority{padding:37px 16px 0 16px;}
.authority dl{width: fit-content; height:68px; clear:both; margin:19px 0 0 0;}
.authority dl:first-child {margin:0px;}
.authority dt{float:left; width:60px; padding:0 10px 0 25px; line-height:56px; letter-spacing: -1.6px;}
.authority dd{float:left;}
.authority ul{float:left; width:54px; margin:0 14px 0 0;}
.authority ul li{position:relative; clear:both; font-size:12px; text-align:center; letter-spacing: -1.2px;}
.authority ul:last-child {margin:0px;}
.authority .set dt{background:url("../../images/ico/icon-cate-auth.png") no-repeat 5px center; background-size:16px 16px;}
.authority .message dt{background:url("../../images/ico/icon-cate-msg.png") no-repeat 5px center; background-size:16px 16px;}

.authority .checkbg{float:left; width:48px; height:48px; border:dashed 1px #ccc; background:#fff; border-radius: 50%; box-sizing: border-box; margin:0 auto;}
.authority .checkbg.on{box-shadow:0px 4px 10px 0 rgba(0, 51, 121, 0.16); border:solid 1px #529bf8;background:#e8f2ff;}
.authority .check{position:absolute; float:right; right:-10px; bottom:0px; width: 16px; height: 16px; box-shadow: 0px 2px 5px 0 rgba(3, 26, 54, 0.2); border: solid 2px #529bf8; border-radius: 20px; background:#529bf8 url("../../images/ico/icon-select.png") no-repeat center center; background-size:14px 14px; display: none;}
.authority .check.on{display: block;}
.authority .img{float:left; width:20px; height:20px; transform: translate(calc(50% + 3px), calc(50% + 3px));}
.authority .img.icon-read{background:url("../../images/ico/icon-read.png") no-repeat center center; background-size:20px 20px;}
.authority .img.icon-modify{background:url("../../images/ico/icon-modify.png") no-repeat center center; background-size:20px 20px;}
.authority .img.icon-msg{background:url("../../images/ico/icon-msg.png") no-repeat center center; background-size:20px 20px;}
.authority .img.icon-mail{background:url("../../images/ico/icon-mail.png") no-repeat center center; background-size:20px 20px;}
.authority .img.icon-msgnone{background:url("../../images/ico/icon-msgnone.png") no-repeat center center; background-size:20px 20px;}

/*메시지 보내기*/
.messageSend {clear: both; margin: 20px 0 0; border-radius:8px; border: solid 1px #ddd; background: #fff; height: 48px; padding: 10px; outline:none; overflow: hidden; max-height: 120px; font-family: AppleSDGothicNeo, Noto sans, '[더존] 본문체 10', sans-serif;}
.messageSend::-webkit-input-placeholder {text-align: left; padding-top: 0px; font-family: AppleSDGothicNeo, Noto sans, '[더존] 본문체 10', sans-serif;}
input:-moz-placeholder {text-align: left;}

/*공유중인 사용자*/
.editShare{clear:both; padding:25px 0 0 0;}
.editShare h2{font-weight:300; background:url("../../images/ico/icon-cate-share.png") no-repeat left center; background-size:16px 16px; padding:0 0 0 19px; margin:0 16px 21px 16px;}
.editShare.line{padding:20px 0 0 0; margin-top: 20px; border-top:1px solid #ddd;}
.editShare .img.icon-read{float:left; width:48px; height:48px;background:#e8f2ff url("../../images/ico/icon-read.png") no-repeat center center; background-size:20px 20px;}
.editShare .img.icon-modify{float:left; width:48px; height:48px;background:#e8f2ff url("../../images/ico/icon-modify.png") no-repeat center center; background-size:20px 20px;}

.editShare dl{margin:0 16px 14px 0px; height:78px;}
.editShare dl dt{float:left; width:48px; padding: 0 16px; font-weight:normal; z-index:10; position:relative; background:#fff;}
.editShare dl dt .text{text-align:center; clear:both; font-size:12px; line-height:30px;}
.editShare dl dd{margin:0 0 0 80px; position:relative; z-index:1;}
.editShare ul{display: -webkit-box;}
.editShare ul li{-webkit-box-orient:horizontal;-webkit-box-align: stretch; padding:0 0px 0 10px; cursor:pointer; }
.editShare .set{border-right:1px solid #ddd;padding: 0 15px 0 0; float:left;}
.editShare .set .img{width:48px; height:48px; border-radius: 50%; border:solid 1px #529bf8; box-sizing: border-box;}
.editShare .title{clear:both; text-align:center; font-size:12px; padding:8px 0 0 0;}

.editShare .userBox{display:block; position:relative; width:56px;}
.editShare .userBox .pic{width:51px !important; margin-left:5px;}
.editShare .userBox .del {position: absolute; right: -15px; bottom: 15px; width: 20px; height: 20px;}
.editShare .userBox .del img {width: 20px; height: 20px; position: relative; left: -10px; top: -13px;}
.editShare .userBox .text{text-align:center; clear:both; font-size:12px; text-overflow:ellipsis;white-space:nowrap;overflow:hidden; letter-spacing:-0.8px; line-height:30px;}

#one_share .separator {width: 100%; border-top: 0px solid #DADADA; border-bottom: 1px solid #DADADA;}
#one_share .separator.thick {height: 8px; background: #f3f3f3; border-top-width: 1px;}
#one_share .shareList li {width: 100%; height: 60px; padding: 0px 16px; border-bottom: 1px solid #DADADA;}
#one_share .shareList li .info {position: relative;top: 50%;transform: translateY(-50%);}
#one_share .shareList li .info img {width: 38px; height: 38px; border-radius: 19px; border: none;}
#one_share .shareList li .info div {display: inline-block; margin-left: 12px;}
#one_share .shareList li .info span {font-size: 18px; letter-spacing: -0.8px; color: #000000; text-align: left;}

/**************************************************
  토스트
**************************************************/
.con.list_view .folder_div {width:100%; clear:both;}
.con.list_view .my_doc .doc_div {width: calc(100% - 8px); clear: both;}
.con.list_view .my_doc .doc_div .preview {display:none;}
.con.list_view .my_doc .doc_div {height:64px !important;}
.con.list_view .my_doc .doc_div .doc_btm {height: 64px; padding-left: 10px; padding-right: 30px; border-radius: 6px; background-color: #ffffff;}
.con.list_view .my_doc .doc_div .doc_btm.on {background-color:#e2f7ff;}
.con.list_view .my_doc .doc_div .check_div {width: 32px; height: 32px; margin-left: calc(50% - 37px); top: 51%; left: 21.5px;}
.con.list_view .my_doc .doc_div .check_div.on {}
.con.list_view .my_doc .doc_div .doc_btm .txt {margin-top:13px;}
.con.list_view .my_doc .doc_div .doc_btm .dat_cond .condi_div {float: none; display: inline-block;}
.con.list_view .my_doc .doc_btm .dat, .con.list_view .my_doc .doc_btm .name {color: #666;}
.con.list_view .my_doc .doc_div .btn_more{top: 18px;}

.template_doc {height: fit-content; padding-bottom: 5px; background: #84cadc; overflow: hidden;}
.template_doc .template_list {max-width: calc(100% - 24px); margin: 0 auto; overflow: visible !important;}
.template_doc .tit {margin: 18px 0 6px 12px; font-size: 16px; color: #fff;}
.template_doc .tit span {padding-left: 20px; background: url(../../images/ico/icon-mdocu-none.png) no-repeat left center; background-size: 15px 15px; margin-left: 5px;}
.template_doc li {display: inline-block; margin: 0px 4px; -webkit-box-orient: horizontal; -webkit-box-align: stretch;}
.template_doc li:last-child {margin-right: 0px;}
.template_doc li .thumb {width: 140px; height: 168px; text-align: center; background: #ffffff; border-radius: 3px; box-shadow: 0px 0px 10px 1px rgba(0, 0, 0, 0.1);}
.template_doc li img {width: 80%; height: auto; margin: 5px 0px;}
.template_doc li p {font-size: 15px; letter-spacing: -1px; color: #ffffff;}

.doc_div .preview {text-align: center;vertical-align: middle;  border-bottom:1px solid #ececec;}
.doc_div {position:relative;border-radius: 6px;box-shadow: 0px 10px 20px 0 rgba(0, 0, 0, 0.1);background-color: #ffffff;}
/* .folder_div:active .folder_div_in {background:#e2f7ff;} */
/* .doc_div:active .doc_btm {background:#e2f7ff;} */

.folder_div .folder_div_in.on {background:#e2f7ff;}
.doc_div .doc_btm.on {background:#e2f7ff;}

.doc_div .doc_btm {height: 44px; background-color: #f4fbff; overflow:hidden;border-radius:0 0 6px 6px; padding: 10px 8px 0px;}
.doc_btm .dat_cond {overflow:hidden;}
.doc_btm .txt {letter-spacing:-0.5px; word-spacing:-1px; font-size:12px;font-weight:bold; text-align:left;text-overflow:ellipsis;overflow:hidden;white-space:nowrap; margin-bottom: 3px;}
.doc_btm .dat, .doc_btm .name {float:left; font-size: 10px;font-weight: 300;letter-spacing: -0.6px; text-align:left;color:#525252; line-height: 1.5;}
.doc_btm .condi_div {float:right;}
.doc_btm .condi_div ul {display: block;}
.doc_btm .condi_div ul li {float: left; margin: 0px 0px 0px 2px !important;}
.doc_btm .condi_div ul li > div {width:16px;height:16px;}
.doc_btm .condi_div ul li .m_star {background:url("../../images/ico/icon-doc-bookmark.png") no-repeat center center;background-size:16px 16px;}
.doc_btm .condi_div ul li .m_human {background:url("../../images/ico/icon-doc-share.png") no-repeat center center;background-size:16px 16px;}
.doc_btm .condi_div ul li .m_human_in {background:url("../../images/ico/icon-doc-share-in.png") no-repeat center center;background-size:16px 16px;}
.doc_btm .condi_div ul li .m_human_out {background:url("../../images/ico/icon-doc-share-out.png") no-repeat center center;background-size:16px 16px;}
.doc_btm .condi_div ul li .m_rock {background:url("../../images/ico/icon-doc-security.png") no-repeat center center;background-size:16px 16px;}
.doc_btm .condi_div ul li .m_eyes {background:url("../../images/ico/icon-doc-readonly.png") no-repeat center center;background-size:16px 16px;}
.doc_btm .condi_div ul li .m_comment {background: url("../../images/ico/icon-doc-cmt.png") no-repeat center center; background-size: 16px 16px;}
.doc_div .btn_more {width:28px; height:28px; display:inline-block; position:absolute; background: url("../../images/btn/btn-more-none.png")no-repeat center center; background-size: 20px 20px; right:0px; top:6px;}
.doc_div .btn_more:active {background:url("../../images/btn/btn-more-sele.png")no-repeat center center;background-size:20px 20px;}
.my_list {overflow: hidden;}
.my_doc {max-width: calc(100% - 24px); margin: 0 auto;}
.my_doc .tit {clear:both; margin: 15px 0px 8px 8px; font-size:16px;letter-spacing:-0.5px;color:#000;padding-left:21px;background:url("../../images/ico/icon-cate-oneffice.png")no-repeat left center; background-size:16px 16px;}
.folder_div {width: 50%; float:left;}
.folder_div_in {margin:4px 4px;height:56px;border-radius: 6px;box-shadow: 0px 5px 9.5px 0.5px rgba(0, 0, 0, 0.07);background-color: #ffffff;}
.folder_div table {width:100%; table-layout: fixed;}
.folder_div .ico_folder {width: 44px; height: 56px; background: url("../../images/ico/icon_folder.png") no-repeat center center; background-size: 20px 20px;}
.folder_div .ico_folder.bookmarked {background-image: url("../../images/ico/icon_folder_bookmark.png")}
.folder_div .ico_more {width:100%; height:56px; background: url("../../images/btn/btn-more-none.png")no-repeat center center; background-size: 20px 20px;}
.folder_div .ico_more:active {background:url("../../images/btn/btn-more-sele.png")no-repeat center center;background-size:20px 20px;}
.my_doc .folder_div .txt {font-size:16px; letter-spacing:-0.6px; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; width:100%;}

.my_doc .doc_div {width: calc(50% - 8px); margin: 4px 4px; float: left;}
.my_doc .doc_div .preview {overflow: hidden; width: 100%;}
.my_doc .doc_div .preview img {width:calc(100% - 20px); margin: 0 auto;}
.my_doc .doc_div .doc_btm {padding-top: 0px; height: 54px;}
.my_doc .doc_div .doc_btm .txt {margin: 8px 0px 6px;}
.my_doc .doc_div .doc_btm  .txt {font-size:16px;}
.my_doc .doc_btm .dat, .my_doc .doc_btm .name {font-size: 12px;}
.my_doc .doc_btm .dat {margin-right: 5px;}
.folder_div.select_on .check_div {display:block;}
.folder_div.copy_on { opacity:0.6;}
.folder_div .check_div {display:none;background:#000b39;opacity:0.2;border-radius:50% 50%;height:32px;width:32px;position:absolute;left:6px; top:13px;}
.folder_div .check_div.on {background:#25b6ec url("../../images/ico/icon-selec-check.png")no-repeat center center;background-size:16px 16px; opacity:1;}
.folder_div table td:first-child {position:relative;}
.doc_div.select_on .check_div {display:block;}
.doc_div.copy_on { opacity:0.6;}
.doc_div .check_div {display: none; background: #000b39; opacity: 0.2; border-radius: 50% 50%; height: 40px; width: 40px; position: absolute; left: 50%; top: calc((100% - 54px) / 2); transform: translate(-50%, -50%);}
