 			$("#from_date").val(getToDay("-","today"));
 		}else{
 			$("#from_date").val(fromDate);
 		}
 		if(toDate == ""){ // 날짜 입력값이 없으면 오늘날짜로 셋팅
 			$("#to_date").val(getToDay("-","today"));
 		}else{
 			$("#to_date").val(toDate);
 		}
 		formTotal.selectDiv.value = selectDiv;
 		formTotal.boardType.value = boardType;
